{
  "name": "chaat",
  "version": "0.2.3",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "electron-start": "electron .",
    "electron-build": "electron-builder",
    "electron-build-win": "electron-builder --win --x64",
    "build": "yarn vue-build && yarn electron-build",
    "build-win": "yarn vue-build && yarn electron-build-win",
    "start": "concurrently \"cross-env BROWSER=none yarn vue-start\" \"wait-on http://localhost:8080 && electron .\"",
    "vue-start": "vue-cli-service serve",
    "vue-build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "build": {
    "mac": {
      "icon": "./build/icon.png"
    },
    "win": {
      "icon": "./build/icon.png"
    },
    "directories": {
      "output": "build"
    }
  },
  "dependencies": {
    "@mdi/font": "^5.8.55",
    "@vue/cli": "^4.5.9",
    "axios": "^0.21.1",
    "core-js": "^3.8.1",
    "dayjs": "^1.9.8",
    "electron-is-dev": "^1.2.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "vue": "^2.6.11",
    "vue-advanced-chat": "^0.6.1",
    "vue-axios": "^3.2.0",
    "vue-router": "^3.4.9",
    "vue-socket.io": "^3.0.10",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
